<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <!-- Chrome, Firefox OS and Opera -->
    <meta name="theme-color" content="#f94b4b">
    <!-- Windows Phone -->
    <meta name="msapplication-navbutton-color" content="#f94b4b">
    <!-- iOS Safari -->
    <meta name="apple-mobile-web-app-status-bar-style" content="#f94b4b">

    <title>@ViewBag.Title Enrique Automóveis - Sua Melhor Opção na Região Serrana </title>
    <meta http-equiv="content-language" content="pt-br">
    <meta name="description" content="A Enrique Automóveis - Petropolis RJ tem a qualidade e tradição no mercado de revenda de automóveis, com diversas linhas, as melhores marcas e fornecedores. As melhores opções disponíveis no segmento automotivo, você encontra aqui." />
    <meta name="keywords" content="venda de carros, loja de carros, loja de carros em petropolis rj, financiar veiculo, comprar veiculos, venda consignada" />

    <link rel="shortcut icon" href="~/images/favicon.ico">
    <link href='http://fonts.googleapis.com/css?family=Grand+Hotel:400' rel='stylesheet' type='text/css'>
    <link href='http://fonts.googleapis.com/css?family=Roboto' rel='stylesheet:100,300,400,500,600,700,800,900' type='text/css'>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.4.0/css/font-awesome.min.css">


    @Styles.Render("~/Content/css")
</head>

<body>

    @Html.Partial("_Topo")

    @RenderBody()


    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/move-top")
    @Scripts.Render("~/bundles/particular")

    @Html.Partial("_Modal")

    <script>

        $(function () {
            $("#slider").responsiveSlides({
                auto: true,
                nav: true,
                speed: 500,
                namespace: "callbacks",
                pager: true,
            });
        });

        $(document).ready(function () {
            $('#horizontalTab').easyResponsiveTabs({
                type: 'default',
                width: 'auto',
                fit: true
            });
        });
    </script>
    <script type="text/javascript">
        $(document).ready(function () {
            $('#horizontalTab').easyResponsiveTabs({
                type: 'default', //Types: default, vertical, accordion
                width: 'auto', //auto or any width like 600px
                fit: true   // 100% fit in a container
            });
        });
    </script>
    <script>
        (function (i, s, o, g, r, a, m) {
            i['GoogleAnalyticsObject'] = r; i[r] = i[r] || function () {
                (i[r].q = i[r].q || []).push(arguments)
            }, i[r].l = 1 * new Date(); a = s.createElement(o),
            m = s.getElementsByTagName(o)[0]; a.async = 1; a.src = g; m.parentNode.insertBefore(a, m)
        })(window, document, 'script', 'https://www.google-analytics.com/analytics.js', 'ga');

        ga('create', 'UA-69936558-4', 'auto');
        ga('send', 'pageview');

    </script>
    @RenderSection("scripts", required: false)

</body>
</html>
